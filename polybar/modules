[module/bspwm]
type = internal/bspwm

pin-workspaces = true
inline-mode = false

enable-click = true
enable-scroll = true
reverse-scroll = true

fuzzy-match = true

ws-icon-0 = I;
ws-icon-1 = II;
ws-icon-2 = III;
ws-icon-3 = IV;
ws-icon-4 = V;
ws-icon-5 = VI;
ws-icon-default = 

format = <label-state>
format-font = 1

label-monitor = %name%

label-separator-background = #2a2e36
label-focused = %name%
label-focused-foreground = ${color.BLUE}
label-focused-background = #2a2e36
label-focused-underline = #2a2e36
label-focused-padding-left = 1
label-focused-padding-right = 1
label-focused-margin = 0

label-occupied = %name%
label-occupied-foreground = ${color.GREEN}
label-occupied-background = #2a2e36 
label-occupied-padding = 1
label-occupied-margin = 0

label-urgent = %name%
label-urgent-foreground = ${color.RED}
label-urgent-background = #2a2e36 
label-urgent-underline = ${color.RED}
label-urgent-padding = 1

label-empty = %name%
label-empty-foreground = ${color.FOREGROUND}
label-empty-padding = 1
label-empty-background = #2a2e36
label-empty-margin = 0

label-separator = " "
label-separator-padding = 0
label-separator-foreground = ${color.ALTBLACK}

;; _-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_-_

