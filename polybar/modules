[module/mem]
type = custom/script
exec = free -m | sed -n 's/^Mem:\s\+[0-9]\+\s\+\([0-9]\+\)\s.\+/\1/p'
format = <label>
format-prefix = "﬙ " 
format-prefix-font = 2
label = %output% MB
label-padding = 0
format-prefix-foreground = #d19a66

